@model List<Usuario>

<h2>Leaderboard - Miembros Destacados</h2>
<table class="leaderboard">
    <thead>
        <tr>
            <th>#</th>
            <th>Nombre</th>
            <th>Peso</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
                <tr>
                    <td>@(i + 1)</td>
                    <td>@Model[i].Nombre</td>
                    <td>@Model[i].Peso kg</td>
                </tr>
        }
    </tbody>
</table>
